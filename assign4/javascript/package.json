{
  "main": "app.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jest"
  },
  "eslintConfig": {
    "globals": {
      "expect": true,
      "jest": true
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 9
    },
    "rules": {
      "eqeqeq": "error",
      "strict": "error",
      "no-var": "error",
      "no-unused-vars": [
        "off"
      ],
      "prefer-const": "error",
      "no-console": "off",
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "single",
        "avoid-escape"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "prompt-sync": "^4.2.0",
    "seedrandom": "^3.0.5"
  }
}
